    agent any
    stages { 
        stage ("pull") {
            steps {
                sh "echo pull successfull"
            }
        }
        stage ("build") {
            steps {
                sh "echo build successfull"
            }
        }
        stage ("test") {
            steps {
                sh "echo test successfull"
            }
        }
        stage ("deploy") {
            steps {
                sh "echo deploy successfull"
            }
        }
    }
}