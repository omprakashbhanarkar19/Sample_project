pipeline {
    agent any
    stages { 
        stage ("pull-stage") {
            steps {
                sh "echo pull successfull"
            }
        }
        stage ("build-stage") {
            steps {
                sh "echo build successfull"
            }
        }
        stage ("deploy-stage") {
            steps {
                sh "echo deploy successfull"
            }
        }
    }
}
